
printf "\n"
printf "================================\n"
printf "    Running $(basename $0):\n"
printf "================================\n"
for l in dir man epub html license pdf readme exe script ; do
    aiorig=$ai
    printf "%26s\n" "--list-$l :"
    printf "%26s\n" "*********************************"
    ./AppRun --list-$l 2>&1
    printf "\n"
done
sleep 1
printf "\n"
