
printf "\n"
printf " ================================\n"
printf "    Running $(basename $0):\n"
printf " ================================\n"
for l in dir man epub html license pdf readme exe script ; do
    printf "%46s\n" "******************************************************************"
    printf "%46s\n" "$ARGV0 --list-$l :   "
    printf "%46s\n" "******************************************************************"
    ./AppRun --list-$l 2>&1
    printf "\n"
done
sleep 1
printf "\n"
