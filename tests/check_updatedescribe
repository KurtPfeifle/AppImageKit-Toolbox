
printf "\n"
printf "================================\n"
printf "    Running $(basename $0):\n"
printf "================================\n"
for ai in appimaged AppImageUpdate appimageupdatetool appimagetool zsync2 zsyncmake2 ; do
    aiorig=$ai
    if [ x$ai == x ] ; then ai=appimagekit-toolbox ; fi
    printf "%46s\n" "******************************************************************"
    printf "%46s\n" "$ARGV0 appimageupdatetool --describe  $ai :   "
    printf "%46s\n" "******************************************************************"
    ./AppRun appimageupdatetool --describe usr/bin/$ai 2>&1
    printf "\n"
done
sleep 3
printf "\n"


